{
  "extends": [
    "next/core-web-vitals",
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "camelcase": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "prettier/prettier": "off",
    "no-console": "off",
    "no-debugger": "off",
    "no-empty": "off",
    "no-mixed-spaces-and-tabs": "off",
    "no-multiple-empty-lines": "off",
    "quotes": "off",
    "semi": "off"
  }
}
